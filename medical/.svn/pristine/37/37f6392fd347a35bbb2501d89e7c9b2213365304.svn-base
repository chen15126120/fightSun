/* 
*  头部
*/
.register-containder{
	width: 1280px;
	z-index: 2;
	margin: 0 auto;
	position: relative;
}
.register { width:220px; height:auto; position:absolute; right:0; top:140px; background:#fff; padding:10px;}
.reg_box { width:220px; height:auto; overflow:hidden;}
.reg_box li { width:220px; float:left; height:25px; margin-top:8px;}
.reg_box li input { height:23px; line-height:23px; width:214px; padding:0 2px;}
.reg_box .reg_btn { width:100%; height:25px; line-height:25px;}
.reg_btn a { width:210px; height:25px; display:block; padding:0 5px;}
.reg_btn a:hover { background:#ff8000;}

.register_new { width:270px; height:auto; display:block; position:absolute; right:40px; top:50px;  background: rgba(255,255,255,0.3);}
.register_ul { width:100%; height:33px; line-height:33px; overflow:hidden;}
.register_ul li { width:135px; height:33px; overflow:hidden;float: left;}

.box_r1 li a.btn-reg,.box_r2 li a.btn-reg{
  display: block;
  line-height: 24px;
  background: #fc9a00;
  padding: 2px 8px;
  width: 70px;
  color: #fff;
  margin: 3px 0 5px 2px;
  float: left;
  border-radius: 10px;
}
.box_r1 li a.btn-reg:hover,.box_r2 li a.btn-reg:hover{
	color: #fc9a00;
	background: #fff;
}
.register_ul li a { width:150px; height:33px; display:block; text-align:center; color:#fff; font-size:18px;background: #999;}
.register_ul li.hover a { background:#666;}
.register_box { width:240px; height:auto; overflow:hidden;background: #999;padding: 15px;}
.box_r2 { width:241px; height:auto; overflow:hidden;}
.box_r1 { width:241px; height:auto;}
.box_r1 span{
	padding-left: 5px;
}
.icheckbox_flat-red{
	float: left;
}
.box_r1 li, .box_r2_ul li {font-size: 14px;color: #fff; height:auto; margin-bottom:12px; overflow:hidden;}
.box_r1 li input, .box_r2_ul li input { width:225px; height:23px; line-height:23px; padding:5px; border:1px solid #fff; float:left;}
.box_r1 li input.hover { border:1px solid #ff4444;}
.box_r2 li input.hover { border:1px solid #ff4444;}
.box_r1 .li_btn, .box_r2_ul .li_btn { width:250px; height:35px; line-height:35px; box-shadow:none; margin-bottom:0; background:none;}
.box_r1 .li_btn a, .box_r2_ul .li_btn a { width:211px; height:35px; line-height:33px; text-align:center; color:#fff; background:#ffa800; border-radius:3px; overflow:hidden; display:block; font-size:16px;}
.box_r1 .li_btn input, .box_r2_ul .li_btn input { border:none;width:240px; height:35px; line-height:33px; text-align:center; color:#fff; background:#ffa800; border-radius:3px; overflow:hidden; display:block; font-size:16px; font-family:'微软雅黑'; cursor:pointer; float: none; padding: 0;}
.box_r1 .li_btn a:hover, .box_r2_ul .li_btn a:hover { background:#f90;}
.box_r1 .li_btn input:hover, .box_r2_ul .li_btn input:hover { background:#f90;border:none;}
.box_r2_ul .li_ico { height:20px; background:none; box-shadow:none; line-height:20px;}
.box_r2_ul .li_ico a:hover { text-decoration:underline;}
.box_r2_ul {  height:auto; padding-bottom: 10px; overflow:hidden; margin:0 auto; margin-top:13px;}
.box_r2_ul li .check_input { width:13px; height:13px;border:none;}

.validate .error{
	font-size: 13px;
	color: #fc9a00;
	margin-top: 5px;
	width: 230px;
}
.tab_youshi, .tab_guide { width:960px; padding:0 10px; height:auto; margin: 20px auto; overflow:hidden; }
#tags_ul { width:100%; height:101px; border-bottom:1px solid #ffa800;}
#tags_ul li { float:left; width:218px; height:100px; border:1px solid #fff; border-bottom:none; padding:0 10px; cursor:pointer;}
#tags_ul li a { width:193px; height:80px; padding:10px 10px; line-height:80px; display:block; color:#00b4d7; cursor:pointer;}
#tags_ul li a .f_80 { width:30%;}
#tags_ul li a .f_14, #tags_ul li a .f_18 { width:70%;}
#tags_ul li a .f_18 { font-weight:bold;}
.f_18 { font-size:18px;}
.f_80 { font-size:80px;}
.f_14 { font-size:14px;}
#tags_ul li a font { float:left;}
#tags_ul .selectTag { border:1px solid #ffa800; height:101px; background:#fff; border-bottom:0;}
#tags_ul .selectTag a { /*border-bottom:1px solid #ffa800;*/ _border-bottom:none; color:#ffa800;}
#tags_full { width:923px; height:140px; border:1px solid #ffa800; border-top:none; padding:20px 10px 10px 25px; font-size:16px; line-height:32px; position:relative;}
#tags_full .btn_a { position:absolute; right:10px; bottom:10px;}
.picGuide1{ background: url(../img/pic_guide_1.png) no-repeat;}
.picGuide2{ background: url(../img/pic_guide_2.png) no-repeat;}
.picGuide3{ background: url(../img/pic_guide_3.png) no-repeat;}
.showStep2,.showStep3{display:none;}
.m_b30 { margin-bottom:30px;}
.f_24 { font-size:24px;}
.f_y { color:#ffa800;}
.btn_a { width:177px; height:28px; line-height:28px; color:#fff; float:right; background:#ffa800; font-size:16px; padding:0 10px;}

.clickStepTag ul{ width:100%; height: 160px;}
.clickStepTag ul li{ width:312px; height: 160px; float:left; cursor: pointer;}
.showStep { height:260px; width:100%; position:relative;}
.showStep .btn_a {position: absolute; right: 10px; bottom: 10px;}
.showStep p { width:900px; height:auto; overflow:hidden; margin-left:20px; margin-bottom:15px;}
.index_input { width:180px; height:18px; padding:2px 5px; line-height:18px; border:1px solid #cccccc; margin-right:10px; float:left;}
.showStep p .dl_a { text-align:center; float:left; height:24px; line-height:24px; padding:0 5px; display:block; background:#FFA800; color:#fff; width:60px;}
.showStep p .dl_a:hover{background:#ff8000;}
.main { width:100%; height:auto; margin:0 auto;}
.bg_page { width:100%; height:auto; margin:0 auto; padding:20px 0 50px 0;}
.m_r20 { margin-right:20px; position: relative; _margin-top:4px;color: #1e1e1e}
.f_l { float:left;}
.f_c { margin:0 auto;}
.f_c { color:#00b4d7;}
.btn_a:hover, .btn_a3:hover { background:#ff8000;color: #fff}

/*最新消息*/
#content #news {
	width: 960px;
  	margin: 0 auto;
	height: 50px;
	background: #fff;
	border: 1px solid #ddd;
}
#content #news .news-title{
	margin: 0 12px;
	height: 51px;
}
#content #news .news-title h1 a{
	font-size: 16px;
	color: #333;
	line-height: 51px;
}
#content #news .news-title h1 i.icon-volume-up{
	color: #df4b4b;
}
#content #news .news-title p a{
	color: #aaa;
	display: block;
	margin-top: 15px;
	font-size: 14px;
}
#content #news .news-list{
	height:30px;
	margin-top: -40px;
	overflow: hidden;
}
#content #news .news-list ul{
	margin: 0px 30px;
}
#content #news .news-list ul li a{
	height: 30px;
	color: #666;
	line-height: 30px;
	font-size: 14px;
}
#content #news .news-list .time{
	margin-left: 30px;
}

/*推荐专家*/
#content #recommend {
	width: 960px;
  	margin: 10px auto;
	height: 440px;
	overflow: hidden;
}
#content #recommend .recommend-title, #content #media .media-title,#content #trophy .trophy-title{
	height: 55px;
}
#content #recommend .recommend-title h1,#content #media .media-title h1,#content #trophy .trophy-title h1{
	width: 108px;
	display: block;
	color: #fc9a00;
	font-size: 20px;
	line-height: 52px;
	border-bottom: 2px solid #fc9a00;
}
#content #recommend .recommend-title p,#content #media .media-title p,#content #trophy .trophy-title p{
	width: 848px;
	line-height: 32px;
	text-align: right;
	border-bottom: 2px dashed #bfbfbf;
}
#content #recommend .recommend-title p a,#content #media .media-title p a,#content #trophy .trophy-title p a{
	color: #fc9a00;
	font-size: 16px;
	display: block;
	width: 90px;
    float: right;
	margin-top: 20px;
}
#content #recommend .recommend-list{
	height: 350px;
	overflow: hidden;
	margin: 10px auto;
	_height:100%;
	position: relative;
}

#content #recommend .doctor{
	margin: 20px;
	width: 300px;
	height: 325px;
	float: left;
	margin: 10px 5px;
	background: #fff;
	border: 1px solid #ddd;
	box-shadow: 1px 6px 6px -4px rgba(0,0,0, 0.3)
}
#content #recommend .doctor table{
	margin: 20px;
}
#content #recommend .doctor-img .img{
	width: 162px;
}
#content #recommend .doctor-img p{
	text-align: center;
}
#content #recommend .doctor-img .text{
	position: relative;
}
#content #recommend .doctor-img i{
	position: relative;
	top: -10px;
	left: 10px;
	font-weight: 500;
	font-size: 85px;
	z-index: 1;
	color: #fc9a00;
	opacity: 0.3;
}
#content #recommend .doctor .keshi{
	color: #fc9a00;
	font-size: 20px;
	z-index: 2;
	left: 23px;
	top: 26px;
	position: absolute;
}
#content #recommend .doctor .name{
	color: #333;
	font-size: 20px;
}
#content #recommend .doctor .title{
	color: #999;
	font-size: 16px;
}

#content #recommend .doctor .doctor-intro p{
	color: #999;
	font-size: 14px;
	height: 42px;
	margin: 20px 0;
}
#content #recommend .doctor .doctor-enter a{
	float: right;
	display: block;
	width: 109px;
	height: 44px;
	background: url('../img/enter.png');
}
#content #recommend .doctor .doctor-enter a:hover{
	background: url('../img/enter-hover.png');
}


/* 媒体*/

#media{
	width: 960px;
  	margin: 0 auto;
	margin-bottom: 20px;
}
#media .media-list{
	width: 100%;
	height: auto;
	overflow: hidden;
	margin-top: 20px;
	margin-bottom: 30px;
}
#media .media-list li{
	width: 140px;
	float: left;
	height: auto;
	margin: 10px;
	list-style: none;
	position: relative;
	_margin: 0 10px 0 8px;
}

/*荣誉*/
#trophy {	
	width: 960px;
  	margin: 0 auto;
	margin-bottom: 60px;
}
#trophy .trophy-list{
	width: 100%;
	height: auto;
	overflow: hidden;
	margin-top: 20px;
	margin-bottom: 30px;
}
#trophy .trophy-list li{
	width: 140px;
	float: left;
	height: auto;
	margin: 0 10px;
	position: relative;
	list-style: none;
	_margin: 0 10px 0 8px;
}
#trophy .trophy-list li img{
	width: 140px;
	height: 75px;
	float: left;
}
#trophy .trophy-list li span{
	width: 140px;
	font-size: 12px;
	float: left;
	color: #828282;
	text-align: center;
	margin-top: 10px;
	text-decoration: underline;
	line-height: 18px;
}
#trophy .trophy-list .trophy-tit p a{
	color: #666;
	font-size: 22px;
}