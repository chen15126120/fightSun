#content .test-top{
	margin-top: 40px;
	height: 65px;
	background: url('../img/test-top.png') no-repeat top center;
}
#content .test-content{
	width: 958px;
	min-height: 300px;
	background: #f9f9f9;
	border-left: 1px solid #ddd;
	border-right: 1px solid #ddd;
}
#content .test-content h1{
	font-size: 24px;
	padding: 10px 20px;
	color: #666;
	background: #f9fafb;
	border-bottom: 4px solid #f5f5f4;
}
#content .test-content h1 a{
	color: #999;
	font-size: 16px;
	padding-left: 30px;
}
#content .test-content .fl-left{
	width: 420px;
	height: 400px;
	margin-left: 30px;
	position: relative;
}
#content .test-content .fl-left h1{
	color: #fc9a00;
	font-size: 20px;
	border: none;
	margin: 20px 0;
}
#content .test-content .fl-left ul{
	margin-left: 20px;
}
#content .test-content .fl-left li{
	line-height: 40px;
}

#content .test-content .fl-left li a{
	color: #666;
	font-size: 18px;
}
#content .test-content .fl-left li a:hover{
	color: #fc9a00;
}
#content .test-content .fl-left p{
	color: #aaa;
	font-size: 14px;
	bottom: 0px;
	left: 20px;
	position: absolute;
}
#content .test-content .img{
	width: 490px;
	height: 500px;
	background: url('../img/test-content.png') no-repeat bottom center;
}
#content .test-content .test-img{
	width: 300px;
	height: 500px;
	background: url('../img/test-img.png') no-repeat bottom center;
}
#content .test-foot{
	margin-bottom: 40px;
	height: 25px;
	background: url('../img/test-foot.png') no-repeat bottom center;
}
#content .test-content .test-subject{
	width: 625px;
	margin: 30px 0 30px 30px;
}

#test .form-group .mid{
	color: #666;
	font-weight: 500;
	font-size: 16px;
	padding-left: 10px;
}
.form-group .iradio_flat-red, .form-group .icheckbox_flat-red{
	margin-left: 35px;
}
#content .test-content .btn-profile{
	width: 100px;
	color: #fff;
	font-size: 18px;
	margin: 35px;
	background: #fc9a00;
}
#content .test-subject p.results{
	font-size: 16px;
	color: #333;
	position: relative;
	line-height: 30px;

}
#content .test-subject span.result{
	width: 50px;
	color: #fff;
	font-size: 14px;
	margin: 0 15px;
	background: #08a3ea;
}