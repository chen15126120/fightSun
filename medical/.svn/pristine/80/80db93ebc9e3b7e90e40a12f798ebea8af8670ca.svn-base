body{
	width: 100%;
	background: #ccc;	/* 背景样式*/
}
.fl-left{
	float: left;
}
.fl-right{
	float: right;
}
a span:hover{
	color: #fc9a00;
}
a{
	color: #ffa800;
}
a:hover{
	color: #ff8000;
}
.clear{
	clear: both;
}
#top,#head,#banner{
	width: 1280px;
	margin: 0 auto;
}
#top .top,#head .head,#content{
	width: 1280px;
	margin: 0 auto;
}
#foot-link .foot-link{
	width: 960px;
	margin: 0 auto;
}
#banner{
	position: absolute;
	top: 0;
	left: 50%;
	margin-left: -640px;
	overflow: hidden;
	z-index: 1;
}
#banner div{
	width: 1280px;
	height: 606px;	
	margin: 0 auto;
	background-size: 100% 100%;
	background-repeat: no-repeat;
}
#content{
	margin-top: 515px;
	background: #fff;
	padding-bottom: 20px;
}
.content{
	width: 960px;	
	margin: 0 auto;	
}
/* 
* 网站通用顶部样式
*/
#top {
	height: 30px;
	line-height: 30px;
	z-index: 9999;
	position: relative;
}
#top .top span{
	font-size: 14px;
	color: #fff;
	margin: 0 20px;
}
#top .top div{
	margin: 0 20px;
}
#top .top div a{
	color: #fff;
	margin-left: 15px;
}
#top .top div a:hover{
	color: #fc9a00;
}
#top .top div span{
	width: 57px;
	margin-top: 2px;
	text-align: center;
	line-height: 25px;
}
#top .top div .login{
	background: #fc9a00;
	display: block;
	border-radius: 20px;
}
#top .top p .register{
	margin-left: 1px;
	background: #08a3ea;
	display: block;
	border-radius: 20px;
}
#top .top div a{
	color: #fff;
	font-size: 14px;
}
/* 
* 网站通用顶部样式结束
*/

/* 
* 网站导航
*/
#head {
	height: 60px;
	position: relative;
	background: rgba(255,255,255,.8);
	z-index: 999;
}
#head .head .logo{
	display: block;
}
#head .head .logo img{
	margin: 8px 0 0 20px;
	height: 43px;
}
#head #nav ul li{
	width: 125px;
	float: left;
	display: block;	
	position: relative;
	text-align: center;
}
#head #nav ul li a{
	display: block;
	color: #333;
	width: 100%;
	font-size: 18px;
	line-height: 60px;
}
#head #nav ul li a:hover,#head #nav ul li a.selected,#head #nav ul li .active{
	background: #fc9a00;
	line-height: 56px;
	color: #fff;
	border-bottom: 4px solid #e58c00;
}
#head #nav ul li .children{
	display: none;
	position:absolute;
	top:60px;
	z-index: 999;
}
#head #nav .children li{
	height: 40px;	
	border-bottom: 2px solid #e58c00;
}
#head #nav .children li a{
	display: block;
	color: #fff;
	background: #fc9a00;
	font-size: 16px;
	line-height: 40px;
}
#head #nav .children li a:hover{
	line-height: 36px;
	border-bottom: 4px solid #e58c00;
}
/* 
* 网站导航结束
*/

#content{
	height: auto;
}
#bread{
	height: 40px;
	line-height: 40px;
	border-bottom: 1px solid #ddd;
}
#bread i.start{
	margin-left: 18px;
	height: 20px;
	border-left: 6px solid #fc9a00;
}
#bread i.icon-angle-right{
	padding-left: 15px;
}
#bread a span, #bread span{
	padding-left: 10px;
	color: #666;
	font-size: 16px;
}
#bread a span:hover{
	color: #fc9a00;
}

#foot{
	height: auto;
}
#foot-link {
	height: 200px;
	background: #faab4e;
}
#foot-link .foot-link ul li{
	width: 33%;
	float: left;
	margin: 20px 0;
}
#foot-link .foot-link ul li h1{
	width: 150px;
	height: 30px;
	font-size: 16px;
	color: #fff;
	margin: 20px 0;
	padding-left: 20px;
	line-height: 30px;
	background: #efa246;
	box-shadow: inset 1px 3px 3px rgba(0,0,0,0.1);
}
#foot-link .foot-link ul li p a{
	color: #fff;
	line-height: 24px;
	font-size: 14px;
	padding-left: 15px;
}
#foot-link .foot-link ul li p a .img{
	position: relative;
  	top: 4px;
}
#foot-link .foot-link ul li p a .large{
	width: 100px;
}
.foot-copyright{
	height: 30px;
	text-align: center;	
	background: #fdf3e8;
}
.foot-copyright a{
	color: #666;
	line-height: 30px;
}

#fast{
	position: fixed;
	bottom: 220px;
	right: 10px;
	z-index: 2;
}
#fast li{
	width: 60px;
	height: 60px;
	border-radius: 30px;	
	margin: 10px 0;
	text-align: center;
}
#fast li a{
	font-size: 14px;
	color: #fff;
	display: block;
}
#fast li a .fast{
	font-size: 24px;
	height: 35px;
	line-height: 40px;
	display: block;
}
#fast li.user{
	background: #fc9a00;
}
#fast li.envelope{
	background: #08a3ea;
}
#fast li.up{
	display: none;
	background: #626262;
}
#fast li.up a .fast{
	font-size: 35px;
	line-height: 60px;
}

/*分页样式*/
#page {
	margin: 0 auto;
	text-align: center;
}
#page ul.pagination {
	display: inline-block;
	padding-left: 0;
	margin: 20px 0;
	border: none;
	min-height: 100px;
	text-align: center;
	border-radius: 4px;
}
#page ul li{
	border: none;
	float: left;
	padding: 0;
}
#page ul li a{
	color: #333;
	margin: 20px 0 20px 8px;
	min-width: 20px;
	height: 20px;
	border: 1px solid #f0f0f0;
	border-radius: 5px;
	display: block;
	font-size: 14px;
}

#page ul li.first a,#page ul li.prev a,#page ul li.next a,#page ul li.last a{
	width: 55px;
}
#page ul li.active a,#page ul li a:hover{
	background: #fc9a00;
	color: #fff;
}
#page ul li.disabled a{
	color: #999;
	cursor: not-allowed;
	background-color: #fff;
	border-color: #ddd;
}
label{
	display: inline-block;
	max-width: 100%;
	margin-bottom: 5px;
	font-weight: 700;
}
.btn{
	display: inline-block;
	padding: 6px 12px;
	margin-bottom: 0;
	font-size: 14px;
	font-weight: 400;
	line-height: 1.42857143;
	text-align: center;
	white-space: nowrap;
	vertical-align: middle;
	-ms-touch-action: manipulation;
	touch-action: manipulation;
	cursor: pointer;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	background-image: none;
	border: 1px solid transparent;
	border-radius: 15px;
}

.form-basic .form-group{
	display: inline-block;
	margin-left: 30px;
	font-size: 16px;
	vertical-align: middle;
}
.form-basic .form-group a{
	display: block;
	width: 80px;
	color: #fff;
	margin-top: 12px;
	border-radius: 20px;
	float: right;
	background: #08a3ea;
	padding: 5px 10px;
	font-size: 16px;
}
.form-basic .normal{
	width: 80px;
	color: #333;
	font-weight: 500;
	font-size: 18px;
}

.form-basic h1{
	font-size: 24px;
	font-weight: 500;
	margin: 20px 30px;
	width: 120px;
	color: #f8f8f8;
	padding: 5px 3px;
	border-radius: 10px;
	text-align: center;
	background: #c7c5be;
}
.form-basic .form-control{
	display: inline-block;
	vertical-align: middle;
	width: 200px;
	height: 25px;
	margin: 10px;
	padding: 6px 12px;
	font-size: 14px;
	line-height: 1.42857143;
	color: #333;
	background-color: #fff;
	background-image: none;
	border: 1px solid #ccc;
	border-radius: 4px;
	-webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
	box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
	-webkit-transition: border-color ease-in-out .15s,-webkit-box-shadow ease-in-out .15s;
	-o-transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
	transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
}
.form-basic .checkbox{
	color: #666;
	line-height: 40px;
	font-size: 14px;
	margin-left: 124px;
}
.form-basic .checkbox a{
	font-weight: 700;
	color: #333;
}
.form-basic .checkbox a:hover{
	color: #08a3ea;
	text-decoration: underline;
}
.input-group{
	position: relative;
	left: 80px;
    top: -35px;
	display: table;
	border-collapse: separate;
}
.form-inline .input-group {
	display: inline-table;
	vertical-align: middle;
}
.input-group .form-control {
	display: table-cell;
	position: relative;
	z-index: 2;
	float: left;
	margin: 0px;
	padding: 6px 12px 6px 6px;
	margin-bottom: 0;
	vertical-align: middle;
	border-left: 0;
	border-top-left-radius: 0;
	border-bottom-left-radius: 0;
}
.input-group-addon:first-child {
	border-right: 0;
	border-top-right-radius: 0;
	border-bottom-right-radius: 0;
}
.form-control:focus {
  border-color: #2b542c;
  -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075),0 0 6px #67b168;
  box-shadow: inset 0 1px 1px rgba(0,0,0,.075),0 0 6px #67b168;
}
.input-group-addon {
	padding: 6px 12px;
	font-size: 18px;
	font-weight: 400;
	line-height: 1;
	color: #333;
	text-align: center;
	background-color: #fff;
	border: 1px solid #ccc;
	border-radius: 4px;
	white-space: nowrap;
	vertical-align: middle;
	display: table-cell;  
	-webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
	box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
}
.form-group .iradio_flat-red,.form-group .icheckbox_flat-red{
	margin: 10px 5px 10px 10px;
}
.form-basic .small{
	width: 100px;
}
.form-basic .md{
	width: 200px;
}
.form-basic .big{
	width: 200px;
}
.form-basic .large{
	width: 550px;
}
.form-basic .mid{
	width: 50px;
}
.form-basic .larger{
	width: 350px;
}
.table{
	width: 100%;
}
.table>thead>tr>th {
	padding: 8px;
	line-height: 1.42857143;
	vertical-align: bottom;
	border-bottom: 2px solid #ddd;
}
/*.table-normal>tbody>tr:nth-of-type(odd) {
  background-color: #f9f9f9;
}*/
.table>tbody>tr>td{
  padding: 8px;
  line-height: 1.42857143;
  vertical-align: top;
  border-top: 1px solid #ddd;
}
.table-normal>tbody>tr>td,.table-normal>thead>tr>th{
	vertical-align: middle;
	border: 1px solid #ddd;
}


.max-overflow{
	width:27em; white-space:nowrap; text-overflow:ellipsis; -o-text-overflow:ellipsis; overflow:hidden;
}
#weixinShowDiv,#weiboShowDiv {
  display: none;
  width: 100px;
  height: 100px;
  padding-top: 5px;
  position: absolute;
  top: 20px;
  right: -10px;
  z-index: 9999;
}
#weiboShowDiv {
	right: 60px;
}
.share_new {width: 240px; height:68px; border-top:1px solid #918f8a; overflow:hidden; padding:10px 15px 15px 5px; margin-top: 10px; color: #fff;font-size: 14px;text-align: center;}
.share_new a{
	display: block;
	float: left;
	margin-left: 10px;
}
.share_new a .qqmail,a.qqmail img{
	margin: 10px 0;
	width: 100px;
}
#login .line,#register .line{
	margin: 10px auto;
	width: 350px;
	height: 1px;
	border-bottom: 1px solid #eee;
}
#login .other,#register .other{
	color: #dc8600;
	font-size: 16px;
	position: relative;
	width: 140px;
	margin: 0 auto;
	top: -33px;
	background: #fff;
	padding: 10px;
}
#register .other{
	text-align: center;
}
#login .others{
	width: 206px;
	margin-top: -20px;
}
#register .others{
	width: 206px;
	margin: -20px auto;
}
#login .qq,#login .wb,#register .qq,#register .wb{
	float: left;
	display: block;
	width: 38px;
	height: 40px;
	margin-left: 10px;
}
#login .qq,#register .qq{
	background: url(../img/qq.png);
}
#login .wb,#register .wb{
	background: url(../img/weibo1.png);
}
.qqmail{
	display: block;
	float: left;
	margin-left: 10px;
}



#star{
	background: #fff;
  	padding: 10px 20px 5px 20px;
}
#star .add-star{
	height: 30px;
	margin: 10px 0;
}
#star span{
	float: left;
	font-size: 18px;
	line-height: 30px;
}
.star_rating,.star_rating1,.star_rating2 {
	float:left;
	display:block;
	list-style:none;
	margin:-1px 0 0 15px; 
	padding:0; 
	position:relative;  
	overflow:hidden;
	font-size:0;
}
.star_rating li,.star_rating1 li,.star_rating2 li{
	padding:0;
	margin:0;
	width: 40px;
	float:left;
}
.star_rating li a,.star_rating1 li a,.star_rating2 li a{
	display:block;
	width:25px;
	height:25px;
	text-decoration:none;
	text-indent:-9000px;
	z-index:20;
	padding:0;
	margin:0;
	background:url(../img/star-white.png) 0 0 repeat-x;
}
.star_rating li a:hover,.star_rating1 li a:hover,.star_rating2 li a:hover{
	background:url(../img/star-yellow.png) 0 25px;
	z-index:2;
	left:0;
}

.comment-content{
	margin: 0px auto;
	padding: 5px;
	margin: 10px 0 40px 0;
	background: #fff;
}
.comment-content textarea{
	height: 100px;
	padding: 10px;
	position: relative;
	background-color: transparent;
	color: #666;
	display: block;
	width: 95%;
	margin: 10px auto;
	font-size: 16px;
	font-family: Arial, Helvetica, sans-serif;
	border: 1px solid #b1b1b1;
	border-radius: 10px;
	overflow: auto;
	-webkit-appearance: none;
}
.comment-content p{
	min-height: 30px;
	padding-top: 10px;
}
.comment-content .button{
	display: block;
	width: 80px;
	height: 35px;
	margin: -10px 12px 0 0;
	float: right;
	line-height: 35px;
	background: #64c528;
	color: white;
	font-size: 20px;
	border-radius: 10px;
	text-align: center;
}

.comment-content img{
	float: left;
	padding-left: 10px;
	width: 20px;
}
.comment-content span{
	display: block;
	float: left;
	line-height: 20px;
  	font-size: 18px;
	padding-left: 10px;
}